{
  "name": "gifbar",
  "version": "1.1.0",
  "repository": "https://github.com/adamweeks/gifbar",
  "description": "GifBar is a gif searching utility that lives in your menubar.",
  "main": "index.js",
  "scripts": {
    "build": "./build.sh",
    "build-webpack": "webpack --progress --profile --colors --display-error-details --display-cached",
    "watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "release-mac": "electron-packager build/ gifbar --platform=darwin --arch=x64 --overwrite --tmpdir=false --out=releases/ --app-version=$npm_package_version --icon=gifbar-icon.icns",
    "release-win": "electron-packager build/ gifbar --platform=win32 --arch=x64 --overwrite --tmpdir=false --out=releases/ --app-version=$npm_package_version",
    "release": "npm run build; npm run release-mac; npm run release-win",
    "electron": "electron build",
    "test": "jest",
    "eslint": "eslint src"
  },
  "author": "Adam Weeks",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.25.0",
    "electron": "^1.6.5",
    "electron-prebuilt": "^1.4.11",
    "es6-shim": "^0.34.0",
    "eslint": "^4.2.0",
    "eslint-plugin-react": "^7.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "minimist": "^1.2.0",
    "react-test-renderer": "^15.6.1",
    "style-loader": "^0.13.1",
    "ts-loader": "^1.3.0",
    "typescript": "^2.0.3",
    "typings": "^0.8.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.0",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "auto-launch-patched": "~5.0.2",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "electron-packager": "^8.6.0",
    "electron-positioner": "^3.0.0",
    "electron-prebuilt": "^1.4.13",
    "es6-shim": "^0.35.3",
    "eslint-plugin-react": "^7.1.0",
    "extend": "^3.0.0",
    "menubar": "^5.2.3",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "style-loader": "^0.13.2",
    "systemjs": "0.19.26",
    "ts-loader": "^1.3.3",
    "typescript": "^2.3.0",
    "webpack": "^1.15.0",
    "webpack-dev-server": "^1.16.3",
    "zone.js": "^0.6.12"
  },
  "jest": {
    "globals": {
      "electron": {
        "remote": {
          "BrowserWindow": true
        }
      }
    },
    "roots": [
      "<rootDir>/src/app/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "identity-obj-proxy"
    },
    "verbose": true
  }
}
